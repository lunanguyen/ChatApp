@model IEnumerable<User>
<link rel="stylesheet" href="~/css/roombutton.css" asp-append-version="true" />

    @foreach(var user in Model) {

    <div>
        <a class="room-button" asp-controller="Home" asp-action="CreatePrivateRoom" asp-route-userId="@user.Id">
            @user.UserName
        </a>
    </div>
    }